#include <unistd.h>

int	my_aff_comb2();
int	loop();
int	display2();

int	my_putchar(char c)
{
  write(1, &c, 1);
}

int	display2(int a,int b,int c,int d)
{
  my_putchar(a);
  my_putchar(b);
  my_putchar(32);
  my_putchar(c);
  my_putchar(d);
  if (a + b + c + d != 171)
    {
      my_putchar(44);
      my_putchar(32);
    }
}

int	loop(int a, int b, int c, int d)
{
  while (a <= 57)
    {
      if (a > c)
	{
	  c++;
	  d = 48;
	}
      else
	{
	  if (a = c)
	    {
	      if (b >= d)
		{
		  d++;
		}
	      
}

int	my_aff_comb2()
{
  int a;
  int b;
  int c;
  int d;
  
  a = 48;
  b = 48;
  c = 48;
  d = 48;

  loop(a,b,c,d);
}

 int	main()
 {
   my_aff_comb2();
 }
