#include <unistd.h>
#include <stdio.h>

void	my_swap(int *a, int *b);

void	my_swap(int *a, int *b)
{
  int	*pa;
  int	*pb;
  int	c;

  pa = a;
  pb = b;
  c = *pa;
  *pa = *pb;
  *pb = c;
}

