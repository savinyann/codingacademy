//void	copy_first_char(char *src,char *dst);
int	array_sum(int *tab,int size);


int	array_sum(int *tab,int size)
{
  int	n;
  int	a;

  n = 0;
  a = 0;
  while (n < size)
    {
      a = a + tab[n];
      //      printf("%d, %d, %d\n",n, tab[n], a);
      n++;
    }
  return (a);
}
